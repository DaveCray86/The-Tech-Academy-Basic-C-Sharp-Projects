@using BasicAutoInsurance.Controllers
@model List<Applicant>

<h2>Admin View</h2>
<br /> 

<table class="table">
    <thead>
        <tr>
            <th scope="col">First Name:</th>
            <th scope="col">Last Name:</th>
            <th scope="col">EMail:</th>
            <th scope="col">Quote Total:</th>
        </tr>
    </thead>
    @foreach (var quote in Model)
    {
        <tbody>
            <tr>
                <td scope="row">
                    @quote.FirstName
                </td>
                <td>@quote.LastName</td>
                <td>@quote.EmailAddress</td>
                <td>$@quote.Total</td>
            </tr>
        </tbody>
    }
</table>